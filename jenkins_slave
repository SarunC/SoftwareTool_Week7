pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Sarun', description: 'Enter your name')
        choice(name: 'CITY', choices: ['Bangkok','Los Angeles', 'Tokyo', 'West Virginia'], description: 'Choose your city')
    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.CITY}\nYou are ${params.Ages} years old"
                
            }
        }
    }
}