pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Sarun', description: 'Enter your name')
        string(name: 'Ages', defaultValue: '20', description: 'Enter your age')
    }
    stages {
        stage('Greet') {
            steps {
                echo "Hello from Slave ${params.NAME}\nYou are ${params.Ages} years old"
                
            }
        }
    }
}