pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                script {
                    def data = readFile(file: 'data.json')
                    echo 'Start Trigger to send name and age to slave'
                    build job: 'slave', parameters: [string(name: 'NAME', value: data.NAME), string(name: 'Ages', value: data.Ages)]
                }
            }
        }
    }
}